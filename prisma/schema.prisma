generator client {
  provider        = "prisma-client-js"
  previewFeatures = []
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URI")
}

// CMS
model Page {
  id           String   @id @default(cuid()) @map("_id")
  title        String
  slug         String
  status       Int
  metadesc     String
  content      String
  dateCreated  DateTime
  dateModified DateTime
}
